{
	"root": true,
	"extends": [
		"wikimedia/server",
		"wikimedia/jsdoc"
	],
	"rules": {
		"camelcase": [
			"error",
			{
				"properties": "never"
			}
		],
		"no-unused-vars": [
			"error",
			{
				"args": "none"
			}
		],
		"es-x/no-hashbang": "off",
		"es-x/no-symbol-prototype-description": "off",
		"yml/no-empty-mapping-value": "warn"
	}
}
